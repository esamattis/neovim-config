
snippet styles
alias ss
options word
  style={styles.${1:name}}

snippet return
alias r
options word
  return ${1}

snippet debugger
alias bp
options word
  debugger;

snippet console.log
alias l
options word
  console.log(${1});

snippet // eslint-disable-next-line
alias el
options word
  // eslint-disable-next-line ${1}

snippet // eslint-disable
alias ed
options word
  // eslint-disable ${1}

snippet // eslint-enable
alias ee
options word
  // eslint-enable ${1}
